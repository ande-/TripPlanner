{
  "name": "travel-planner",
  "version": "0.0.1",
  "main": "js/app.js",
  "dependencies": {
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.13.0",
    "bootstrap": "^3.3.5"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
